<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:width="30dp"
        android:height="20dp"
        >
        <shape android:shape="rectangle">
            <solid android:color="#479" />
            <corners android:radius="2dp" />

        </shape>
    </item>

    <item
        android:width="15dp"
        android:height="17dp"
        android:gravity="top|center"
        >
        <shape android:shape="rectangle">
            <solid android:color="#974" />
            <corners android:radius="3dp" />

        </shape>
    </item>
    <item
        android:width="26dp"
        android:height="16dp"
        android:gravity="left|bottom"
        >
        <shape android:shape="rectangle">
            <solid android:color="#794" />
            <corners android:radius="5dp" />

        </shape>
    </item>
    <item
        android:width="10dp"
        android:height="18dp"
        android:gravity="right|center"
        >
        <shape android:shape="rectangle">
            <solid android:color="#497" />
            <corners android:radius="15dp" />

        </shape>
    </item>
    <item
        android:width="15dp"
        android:height="10dp"
        android:gravity="bottom|center">
        <shape android:shape="rectangle">
            <solid android:color="#947" />
            <corners android:radius="3dp" />

        </shape>
    </item>
</layer-list>

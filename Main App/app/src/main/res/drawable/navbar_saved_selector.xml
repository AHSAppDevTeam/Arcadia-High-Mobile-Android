            </item>
            <item android:drawable="@drawable/navbar_saved_ic_active"
                android:width="24dp"
                android:height="24dp"
                android:gravity="center"

                />
        </layer-list>
    </item>
    <item>
        <layer-list>
            <item
                android:width="30dp"
                android:height="30dp">
                <shape>

                </shape>
            </item>
            <item android:drawable="@drawable/navbar_saved_ic"
                android:width="24dp"
                android:height="24dp"
                android:gravity="center"
                />
        </layer-list>
    </item>
</selector>
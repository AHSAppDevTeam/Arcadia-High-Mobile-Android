<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <rotate
            android:fromDegrees="160"
            android:pivotX="50%"
            android:pivotY="50%"
            android:toDegrees="0">
            <shape
                android:shape="line"
                android:top="1dip">
                <stroke
                    android:width="3dp"
                    android:color="?attr/titleColor"/>
            </shape>
        </rotate>
    </item>
    <item android:drawable="@drawable/article_appbar_saved_ic"
        android:width="@dimen/navBarInnerIcon_height"
        android:height="@dimen/navBarInnerIcon_height"
        android:gravity="center"/>
</layer-list>
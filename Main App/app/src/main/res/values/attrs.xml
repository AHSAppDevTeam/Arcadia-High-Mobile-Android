<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name = "titleColor" value = "color"/>
    <attr name = "bodyTextColor" value = "color"/>
    <attr name = "authorTextColor" value = "color"/>
    <attr name = "bgColor" value="color"/>
    <attr name = "bgColorAccent" value="color"/>
    <attr name = "navBarIconColor" value="color"/>
    <attr name = "navBarIconColor_active" value="color"/>
    <attr name = "navBarIconColorAccent_active" value="color"/>

    <declare-styleable name="TextViewSelector">
        <attr name="android:background"/>
        <attr name="backgroundInactive" format="reference"/>
        <attr name="android:textColor"/>
        <attr name="textColorInactive" format="color"/>
        <attr name="selected" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="IconButton">
        <attr name="android:background"/>
        <attr name="text" format="string" />
        <attr name="icon" format="reference" />
    </declare-styleable>

</resources>